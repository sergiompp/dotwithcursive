! special
*.foreground:   #000000
*.background:   #ffffff
*.cursorColor:  #000000

*color0: #353535
*color1: #fc5e61
*color2: #8ae3a4
*color3: #fada6e
*color4: #537ae6
*color5: #be7aef
*color6: #68c1b5
*color7: #dddddd
*color8: #eeeeee
*color9: #f79c9e
*color10: #bddfc7
*color11: #e5d08b
*color12: #9cb4f7
*color13: #c89fe5
*color14: #8bc4bc
*color15: #ffffff

*font: xft:Victor Mono:size=14:Italic

*lineSpace:             5
*letterSpace:           1
*geometry:              40x10
*internalBorder:        40
*scrollBar:             false


! xft fonts settings
Xft.autohint:           0
Xft.lcdfilter:          lcddefault
Xft.hinting:            1
Xft.hintstyle:          hintslight
Xft.antialias:          1
Xft.rgba:               rgb

URxvt.perl-ext-common: config-reload

URxvt.cursorBlink: 1
URxvt.cursorUnderline: 1
